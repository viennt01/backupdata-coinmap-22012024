{
  "extends": ["next/core-web-vitals", "prettier", "plugin:react/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["warn"],
    "react/prop-types": "off",
    "react/react-in-jsx-scope": 0,
    "react/jsx-uses-react": 0,
    "no-unused-vars": "warn",
    "@next/next/no-img-element": 0
  }
}
